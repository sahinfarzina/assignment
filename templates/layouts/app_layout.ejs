        <%- defineContent('page_js')%>
        <script>
        $(function () {
            $(".removerecord").bind('click',function(){
                if(confirm("Are you sure,\nYou want to remove the record?")){
                    return true;
                }
                return false;
            });
            $(".block").bind('click',function(){
                if(confirm("Are you sure,\nYou want to block this record?")){
                    return true;
                }
                return false;
            });
            $(".blockusr").bind('click',function(){
                if(confirm("Are you sure,\nYou want to block this user?")){
                    return true;
                }
                return false;
            });
            $('#Cancel').click(function(){
                $('#siteFrm')[0].reset();
                //$('select option:selected').removeAttr('selected');
                //$('#gradeNo').html('<option value="" ></option>');
            });
            $(document).ready(function(){
                menu_selections()
                
            })
            function menu_selections(){
                let url = location.href;
                $.each($(".sidebar-menu li a"), function(i, item){
                    console.log('hi')
                    let matchFound = url.match(new RegExp($(item).attr('href'),"i"))
                    if(matchFound){
                        $(item).parent().addClass('active')
                        return false;
                    }
                })
            }   
            
        })
        
        
            
        $(function () {
            $('#example1').DataTable()
            $('#example2').DataTable({
            'paging'      : true,
            'lengthChange': false,
            'searching'   : false,
            'ordering'    : false,
            'info'        : true,
            'autoWidth'   : false
            })
        })
        var page_url = window.location;
        $('ul.treeview-menu a').filter(function () {
            return page_url.toString().includes(this.href.toString()); 
        }).parentsUntil(".sidebar-menu > .treeview-menu").addClass('active');
        </script>
